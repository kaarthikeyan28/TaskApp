<div class="body-pane" style="margin-top:52px">
	<div class="form-horizontal ela-pt-20 ela-mb-40">
		<div class="form-group">
			<label class="col-md-3 control-label erp-pt-null">
				Devices
			</label>
			<div class="col-md-5 ela-pr-null">
				{{bs-select-2 data=fileTypeFilter}}
			</div>
		</div>
		<div class="form-group ela-mb-10 ela-mt-15">
			<div class="col-md-8 ela-pr-null col-md-offset-3">
				<div class="checkbox ela-pt-null">
					{{check-box isChecked=usedefault }}
					<span class="ela-space-1"></span>
					Use existing credential
					{{icon-set json=usernameHelpIcon}}
				</div>
			</div>

		</div>
		<div class="form-group">
			<label class="control-label col-md-3 ela-pr-null">
				<span class="required">*</span>Username
			</label>
			<div class="col-md-5 ela-pr-null">
				<input type="text" class="form-control pull-left">
			</div>
		</div>
		<div class="form-group">
			<label class="control-label col-md-3 ela-pr-null">
				<span class="required">*</span>Password
			</label>
			<div class="col-md-5 ela-pr-null">
				<input type="password" class="form-control pull-left">
			</div>
		</div>
		<div class="form-group ela-pt-10">
			<div class="col-md-offset-3 col-md-8">
				<button class="btn btn-primary" {{action "onSave"}}>
					Save
				</button>
				<button class="btn btn-default btn-left-space" {{action "onBack"}}>Cancel</button>
			</div>
		</div>
	</div>
</div>